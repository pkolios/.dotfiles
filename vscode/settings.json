{
    "breadcrumbs.enabled": false,
    "editor.fontFamily": "'Victor Mono', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": false,
    "editor.cursorStyle": "block",
    "editor.formatOnSave": true,
    "editor.suggestSelection": "first",
    "editor.minimap.enabled": false,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.dragAndDrop": false,
    "editor.lineNumbers": "off",
    "editor.tabCompletion": "on",
    "editor.glyphMargin": false,
    "explorer.enableDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "explorer.autoReveal": false,
    "errorLens.addAnnotationTextPrefixes": true,
    "errorLens.fontStyleItalic": true,
    "git.defaultCloneDirectory": "",
    "git.enabled": false,
    "problems.showCurrentInStatus": true,
    "python.pythonPath": "/usr/local/opt/python@3.8/bin/python3",
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "python.jediEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.testing.promptToConfigure": false,
    "python.formatting.provider": "black",
    "python.sortImports.path": "/usr/local/bin/isort",
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "scm.countBadge": "off",
    "scm.providers.visible": 0,
    "scm.alwaysShowProviders": false,
    "task.autoDetect": "off",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.visual": "line",
    "vim.useSystemClipboard": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.startupEditor": "newUntitledFile",
    "window.closeWhenEmpty": true,
    "workbench.activityBar.visible": false,
    "window.openFilesInNewWindow": "default",
    "python.dataScience.jupyterServerURI": "local::",
    "workbench.colorTheme": "Sonokai Andromeda",
}