{
  "editor.fontFamily": "'Victor Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.cursorStyle": "block",
  "editor.formatOnSave": true,
  "editor.suggestSelection": "first",
  "editor.minimap.enabled": false,
  "editor.suggest.localityBonus": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.dragAndDrop": false,
  "editor.lineNumbers": "off",
  "editor.tabCompletion": "on",
  "editor.glyphMargin": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "explorer.enableDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "explorer.autoReveal": false,
  "errorLens.addAnnotationTextPrefixes": true,
  "errorLens.fontStyleItalic": true,
  "git.defaultCloneDirectory": "",
  "git.enabled": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.pythonPath": "/usr/local/opt/python@3.8/bin/python3",
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "python.jediEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.testing.promptToConfigure": false,
  "python.formatting.provider": "black",
  "python.sortImports.path": "/usr/local/bin/isort",
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "scm.countBadge": "off",
  "scm.providers.visible": 0,
  "scm.alwaysShowProviders": false,
  "task.autoDetect": "off",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.visual": "line",
  "vim.useSystemClipboard": true,
  "vim.leader": ",",
  "workbench.startupEditor": "newUntitledFile",
  "window.closeWhenEmpty": true,
  "window.openFilesInNewWindow": "default",
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Sonokai Andromeda",
}
